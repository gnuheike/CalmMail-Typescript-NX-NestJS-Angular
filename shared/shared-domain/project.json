{
  "name": "shared-domain",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared/shared-domain",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/shared/shared-domain",
        "main": "shared/shared-domain/index.ts",
        "tsConfig": "shared/shared-domain/tsconfig.lib.json",
        "assets": [
          "shared/shared-domain/*.md"
        ],
        "format": [
          "cjs"
        ],
        "generateLockfile": false
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "shared/shared-domain/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
