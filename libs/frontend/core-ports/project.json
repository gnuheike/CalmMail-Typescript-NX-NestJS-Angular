{
  "name": "frontend-core-ports",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/core-ports",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/frontend/core-ports",
        "main": "libs/frontend/core-ports/index.ts",
        "tsConfig": "libs/frontend/core-ports/tsconfig.lib.json",
        "assets": [
          "libs/frontend/core-ports/*.md"
        ],
        "format": [
          "cjs"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/frontend/core-ports/jest.config.ts"
      }
    }
  }
}
